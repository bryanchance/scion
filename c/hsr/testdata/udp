ip table add 1
loop create
set interface ip table loop0 1
set int ip address loop0 192.168.1.1/8
set int state loop0 up

packet-generator new {
    name udp
    limit 8096
    rate 1e5
    node ip4-input
    size 100-100
    interface loop0
    no-recycle
    data {
        UDP: 192.168.1.2 -> 192.168.1.1
        UDP: 4321 -> 30046
            length 80
            hex 0x00410048070405110001ff00000000020001ff00000000010101010102020202016f00000000010200050160046ee65d0004005006FACADE3333444400100000deadbeefcafebabe
    }
}
