--- # Topology for ipscraper_acceptance test.
defaults:
  zookeepers:
    1:
      addr: 127.0.0.1
ASes:
  # IPScraper runs in this AS.
  "1-ff00:0:110":
    core: true
    mtu: 1400
  # IPProvider runs in this AS.
  "1-ff00:0:111":
    cert_issuer: 1-ff00:0:110
links:
  - {a: "1-ff00:0:110#1", b: "1-ff00:0:111#1", linkAtoB: CHILD}
CAs:
  CA1-1:
    ISD: 1
    commonName: CA1-1
